{
	"version": 2,
	"alias": [
		"beta.dotwatcher.cc",
		"www.dotwatcher.cc",
		"dotwatcher.cc",
		"dotwatcher"
	],
	"regions": [
		"sfo1",
		"bru"
	],
	"builds": [
		{
			"src": "server.js",
			"use": "@now/next"
		}
	],
	"env": {
		"CONTENTFUL_ACCESS_TOKEN": "@contentful-api-token",
		"PUSHER_APP_ID": "@pusher-app-id",
		"PUSHER_KEY": "@pusher-key",
		"PUSHER_SECRET": "@pusher-secret",
		"MAILCHIMP": "@mailchimp",
		"ANALYTICS": "@analytics",
		"SCRAPEY_API_KEY": "@scrapey-api-key"
	}
}
